{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "es2022",
		"module": "node16",
		"moduleResolution": "node16",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./out",
		"rootDir": ".",
		"sourceMap": true
	},
	"include": ["**/*.ts"]
}
